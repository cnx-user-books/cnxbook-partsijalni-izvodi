            </m:mrow>
            <m:annotation encoding="StarMath 5.0"> size 12{A rSub { size 8{2} }  left ( -  sqrt { {  {2}  over  {"11"} } } ,` {  {1}  over  {2} }  sqrt { {  {2}  over  {"11"} } } ,` - 2 sqrt { {  {2}  over  {"11"} } }  right )} {}</m:annotation>
