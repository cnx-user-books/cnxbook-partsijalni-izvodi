                                  <m:mrow>
                                    <m:mn>0</m:mn>
                                  </m:mrow>
                                </m:mstyle>
                              </m:msub>
                              <m:mi>,</m:mi>
                              <m:msub>
                                <m:mi>z</m:mi>
