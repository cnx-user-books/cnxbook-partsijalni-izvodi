                              <m:mo stretchy="false">(</m:mo>
                              <m:msub>
                                <m:mi>x</m:mi>
                                <m:mstyle fontsize="8pt">
                                  <m:mrow>
                                    <m:mn>0</m:mn>
                                  </m:mrow>
                                </m:mstyle>
                              </m:msub>
                              <m:mi>,</m:mi>
                              <m:msub>
                                <m:mi>y</m:mi>
